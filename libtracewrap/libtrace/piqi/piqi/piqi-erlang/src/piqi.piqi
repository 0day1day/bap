
.include [ .module piqic/piqi-erlang ]


% remove the "piqi" prefix from generated Erlang type and record names
.erlang-type-prefix ""


% now, with prefix being removed, we need to define custom Erlang names for some
% types
.extend [
    .name function
    .erlang-name "func"
]


.extend [
    .name any
    .erlang-name "piqi_any"
]

